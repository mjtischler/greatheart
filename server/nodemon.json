{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "app.js",
    "routes",
    "db/db-access.js",
    "public",
    "views"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
