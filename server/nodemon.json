{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "Public/posts/images",
    "logs",
    "README.md"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "app.js",
    "config",
    "routes",
    "db/db-access.js",
    "public",
    "views",
    "session"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
